<?xml version="1.0"?>
<package >
  <metadata>
    <id>PublicApiGenerator</id>
    <version>$version$</version>
    <title>Public Api Generator</title>
    <authors>Jake Ginnivan</authors>
    <owners>Jake Ginnivan</owners>
    <projectUrl>https://github.com/JakeGinnivan/ApiApprover</projectUrl>
    <licenseUrl>https://github.com/JakeGinnivan/ApiApprover/blob/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This library simply returns your public API as a string. You can use this in approval style tests or for documentation</description>
    <copyright>Copyright Jake Ginnivan 2015</copyright>
    <tags>semanticversioning versioning api</tags>
    <releaseNotes>
      v6.0 Breaking changes:
      - The async keyword doesn't influence the public API and therefore is no longer treated as part of the public API.    
      v5.0 Breaking changes:
      - Converted to source only package
      - Invariant Culture used for string.Format
      - Custom attributes are properly taken into account for set methods
    </releaseNotes>
    <dependencies>
      <dependency id="Mono.Cecil" version="0.10.0-beta1-v2" />
    </dependencies>
  </metadata>
  <files>
    <file src="ApiGenerator.cs" target="content\App_Packages\PublicApiGenerator.$version$\" />
  </files>
</package>
